<%# toc %>
<%if(toc(page.content).length!==0){%>
  <%- partial('_partial/toc') %>
<%}%>
<div class="post">
  <div class="post-header">
    <h1 class="post-title"><%- page.title %></h1>
    <p class="post-meta"><%- date(page.date, "YYYY-MM-DD HH:mm:ss") %></p>
  </div>
  <div class="post-content">
    <article>
      <%- page.content %>
    </article>
  </div>
  <div class="post-footer">
    <a href="javascript:window.history.back();">
      cd ..
    </a>
    <br>
    <a href="<%- config.root %>">
      cd /
    </a>
  </div>
  <div class="post-license">
    <span>
      <a href="https://creativecommons.org/licenses/by-nc-sa/4.0/" target="_blank" style="color: inherit;">
        CC BY-NC-SA 4.0
      </a>
      2021-PRESENT Â© <%= config.author %>
    </span>
    <div></div>
  </div>
  <div class="post-comment">
    <div id="disqusjs"></div>
  </div>
</div>
